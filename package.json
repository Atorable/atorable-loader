{
    "name": "atorable-loader",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "version": "0.0.9",
    "description": "Webpack loader for turning files into Magnet links",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "source": "src/index.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean-dist": "rimraf dist/*",
        "build": "npm run clean-dist && npx tsc --project tsconfig.json",
        "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Atorable/atorable-loader"
    },
    "keywords": [
        "webtorrent",
        "react",
        "webpack",
        "tor",
        "img",
        "video",
        "loader"
    ],
    "dependencies": {
        "create-torrent": "^5.0.1",
        "form-data": "^4.0.0",
        "loader-utils": "^3.2.0",
        "node-fetch": "^2.6.6",
        "parse-torrent": "^9.1.4"
    },
    "devDependencies": {
        "@types/create-torrent": "^5.0.0",
        "@types/loader-utils": "^2.0.3",
        "@types/node": "^16.11.12",
        "@types/node-fetch": "^2.5.12",
        "@types/parse-torrent": "^5.8.4",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.3"
    },
    "author": "Serge Thompson",
    "license": "ISC"
}
